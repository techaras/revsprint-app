generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


// Just one simple table to test the connection
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

